<h1>Clients</h1>


<%= link_to 'New client', new_client_path%>
<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Last name</th>
        <th></th>
    </tr>
    <thead>
    <tbody>
    <% @clients.each do |client| %>
        <tr>
            <td><%=client.name%></td>
            <td><%=client.last_name%></td>
            <td><%=link_to 'Edit', edit_client_path(client)%></td>
        </tr>
    <% end %>
    </tbody>
</table>